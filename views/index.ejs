<%- include('./partials/navbar.ejs') %>


<!-- Testimonials -->
<section id="home">
  <div class="donut-container">
    <div id="carousel-try" class="carousel slide" data-ride="carousel" data-interval="3500">
      <div class="carousel-inner">
        <!--ini tempat dari ISI carouselnya -->

        <div class="carousel-item active" style="background-image: url('./img/donut14.png'); background-position-y: 30%;">
        </div>

        <div class="carousel-item" style="background-image: url('./img/donut14.png'); background-position-y: bottom;">
        </div>

        <div class="carousel-item" style="background-image: url('./img/donut16.png'); background-position-y: 70%;">
        </div>

        <div class="carousel-item" style="background-image: url('./img/donut17.png'); background-position-y: 50%;">
        </div>
      </div>

      <a class="carousel-control-prev" href="#carousel-try" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
      </a>

      <a class="carousel-control-next" href="#carousel-try" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
      </a>

    </div>
  </div>
</section>


<div class="donut-container" id="menu">
  <h2 class="donut-header">ORIGINAL SAGA</h2>

  <%- include('./partials/donutGrid', {donuts: donuts, large: true}) %>
</div>

